<web-app>
	<filter>
		<filter-name>guiceFilter</filter-name>
		<filter-class>com.google.inject.servlet.GuiceFilter</filter-class>
	</filter>
	
	<filter-mapping>
		<filter-name>guiceFilter</filter-name>
		<url-pattern>/add</url-pattern>
	</filter-mapping>
	
	<listener>
		<listener-class>com.web2.Listener.MyListener</listener-class>
	</listener>

</web-app>